<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sunbox.dao.mapper.BaseReleaseAppsConfigDataMapper">
    <resultMap id="BaseResultMap" type="com.sunbox.domain.BaseReleaseAppsConfig">
        <!--@mbg.generated-->
        <!--@Table base_release_apps_config-->
        <id column="release_version" jdbcType="VARCHAR" property="releaseVersion" />
        <result column="app_name" jdbcType="VARCHAR" property="appName" />
        <result column="app_config_classification" jdbcType="VARCHAR" property="appConfigClassification" />
        <result column="app_config_file" jdbcType="VARCHAR" property="appConfigFile" />
        <result column="sort_no" jdbcType="INTEGER" property="sortNo" />
        <result column="is_delete" jdbcType="INTEGER" property="isDelete" />
        <result column="createdby" jdbcType="VARCHAR" property="createdby" />
        <result column="created_time" jdbcType="TIMESTAMP" property="createdTime" />
    </resultMap>
    <sql id="Base_Column_List">
        <!--@mbg.generated-->
        release_version, app_name, app_config_classification, app_config_file, is_delete, sort_no,
        createdby, created_time
    </sql>
    <select id="selectAll" resultType="java.lang.String">
        select  distinct (app_config_classification)
        from   base_release_apps_config
      where 1=1
        <if test="releaseVersionList!=null and releaseVersionList.size>0 ">
            and release_version in
            <foreach collection="releaseVersionList" index="index" open="(" close=")" separator="," item="item">
                #{item}
            </foreach>
        </if>
    </select>

</mapper>